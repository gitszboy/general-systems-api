	<parameter name="quo_code_param" class="java.lang.Long"/>
	<queryString>
		<![CDATA[select prp_surname,
	   prp_email,
       prp_dob,
       CASE WHEN quo_freq_of_payment ='M' THEN 'Monthly'
			WHEN quo_freq_of_payment ='A' THEN 'Annually'
            WHEN quo_freq_of_payment ='S' THEN 'Semi-Annually'
            WHEN quo_freq_of_payment ='Q' THEN 'Quarterly'
            WHEN quo_freq_of_payment ='F' THEN 'Single Payment'
		END As quo_freq_of_payment,
       quo_term,
       COALESCE (quo_tot_sa, 0) quo_tot_sa,
       COALESCE (quo_tot_premium, 0) quo_tot_premium,
       COALESCE (rb_amt, 0) rb_amt,
       COALESCE (tb_amt, 0) tb_amt,
       agn_name,
       COALESCE (quo_medicals, 'N') quo_medicals,
       quo_effective_date,
       prod_desc
from quotations INNER JOIN proposers ON quo_prp_code = prp_code
	 INNER JOIN Products ON quo_prod_code = prod_code
	 LEFT OUTER JOIN (SELECT qbn_quo_code rb_quo_code, qbn_amt rb_amt FROM lmsagentdb.quotation_bonuses where qbn_type = 'R')rb ON quo_code = rb.rb_quo_code
	 LEFT OUTER JOIN (SELECT qbn_quo_code tb_quo_code, qbn_amt tb_amt FROM lmsagentdb.quotation_bonuses where qbn_type = 'T')tb ON quo_code = tb.tb_quo_code
     LEFT OUTER JOIN agents ON quo_agn_code = agn_code
where quo_code = $P{quo_code_param}]]>
	</queryString>
